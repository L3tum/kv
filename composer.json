    "spiral/roadrunner": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "spiral/core": "^1.0",
    "doctrine/inflector": "^1.3",
    "spiral/code-style": "^1.0",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "vendor-dir": "vendor_php"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\KV\\": "src/"
    },
    "files": [
      "src/functions.php"
